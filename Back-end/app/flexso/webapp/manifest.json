{
  "_version": "1.59.0",
  "sap.app": {
    "id": "flexso",
    "type": "application",
    "i18n": "i18n/i18n.properties",
    "applicationVersion": {
      "version": "0.0.1"
    },
    "title": "{{appTitle}}",
    "description": "{{appDescription}}",
    "resources": "resources.json",
    "sourceTemplate": {
      "id": "@sap/generator-fiori:basic",
      "version": "1.13.2"
    },
    "dataSources": {
      "mainService": {
        "uri": "/odata/v4/event/",
        "type": "OData",
        "settings": {
          "annotations": [],
          "odataVersion": "4.0"
        }
      },
      "v2model": {
        "uri": "/odata/v2/event/",
        "type": "OData",
        "settings": {
          "annotations": [],
          "odataVersion": "2.0"
        }
      }
    }
  },
  "sap.ui": {
    "technology": "UI5",
    "deviceTypes": {
      "desktop": true,
      "tablet": true,
      "phone": true
    }
  },

  "sap.ui5": {
    "flexEnabled": true,
    "dependencies": {
      "minUI5Version": "1.123.1",
      "libs": {
        "sap.m": {},
        "sap.ui.core": {},
        "sap.f": {}
      }
    },
    "contentDensities": {
      "compact": true,
      "cozy": true
    },
    "models": {
      "i18n": {
        "type": "sap.ui.model.resource.ResourceModel",
        "settings": {
          "bundleName": "flexso.i18n.i18n"
        }
      },
      "": {
        "dataSource": "mainService",
        "preload": true,
        "settings": {
          "synchronizationMode": "None",
          "operationMode": "Server",
          "autoExpandSelect": true,
          "earlyRequests": true
        }
      }
    },
    "resources": {
      "css": [
        {
          "uri": "css/style.css"
        }
      ]
    },
    "routing": {
      "config": {
        "routerClass": "sap.m.routing.Router",
        "viewType": "XML",
        "async": true,
        "viewPath": "flexso.view",
        "controlAggregation": "pages",
        "controlId": "app",
        "clearControlAggregation": false
      },
      "routes": [
        {
          "name": "RouteApp",
          "pattern": ":?query:",
          "target": ["TargetApp"]
        },
        {
          "name": "RouteEvent",
          "pattern": "Event/{eventID}",
          "target": ["TargetEvent"]
        },
        {
          "name": "EventList",
          "pattern": "events",
          "target": ["TargetEventList"]
        },
        {
          "name": "CreateEvent",
          "pattern": "create-event",
          "target": ["TargetCreateEvent"]
        },
        {
          "name": "eventManager",
          "pattern": "eventManager",
          "target": ["TargetEventManager"]
        }
      ],
      "targets": {
        "TargetApp": {
          "viewType": "XML",
          "viewPath": "flexso.view",
          "viewName": "app",
          "viewId": "app"
        },
        "TargetEvent": {
          "viewType": "XML",
          "viewPath": "flexso.view",
          "viewName": "Account",
          "viewId": "Account"
        },
        "TargetEventList": {
          "viewType": "XML",
          "viewPath": "flexso.view",
          "viewName": "EventList",
          "viewId": "EventList",
          "viewLevel": 1
        },
        "TargetCreateEvent": {
          "viewType": "XML",
          "viewPath": "flexso.view",
          "viewName": "CreateEvent",
          "viewId": "CreateEvent",
          "viewLevel": 2
        },
        "TargetEventManager": {
          "viewType": "XML",
          "viewPath": "flexso.view",
          "viewName": "EventManager",
          "viewId": "EventManager",
          "viewLevel": 2
        }
      }
    },
    "rootView": {
      "viewName": "flexso.view.App",
      "type": "XML",
      "async": true,
      "id": "app"
    }
  }
}
