{
  "_version": "1.59.0",
  "sap.app": {
    "id": "flexso",
    "type": "application",
    "i18n": "i18n/i18n.properties",
    "applicationVersion": {
      "version": "0.0.1"
    },
    "title": "{{appTitle}}",
    "description": "{{appDescription}}",
    "resources": "resources.json",
    "sourceTemplate": {
      "id": "@sap/generator-fiori:basic",
      "version": "1.13.2"
    },
    "dataSources": {
      "mainService": {
        "uri": "/odata/v4/event/",
        "type": "OData",
        "settings": {
          "annotations": [],
          "odataVersion": "4.0"
        }
      },
      "v2model": {
        "uri": "/odata/v2/event/",
        "type": "OData",
        "settings": {
          "annotations": [],
          "odataVersion": "2.0"
        }
      }
    }
  },
  "sap.ui": {
    "technology": "UI5",
    "deviceTypes": {
      "desktop": true,
      "tablet": true,
      "phone": true
    }
  },

  "sap.ui5": {
    "flexEnabled": true,
    "dependencies": {
      "minUI5Version": "1.123.1",
      "libs": {
        "sap.m": {},
        "sap.ui.core": {},
        "sap.f": {},
        "sap.suite.ui.generic.template": {},
        "sap.ui.comp": {},
        "sap.ui.generic.app": {},
        "sap.ui.table": {},
        "sap.ushell": {},
        "sap.uxap": {}
      }
    },
    "contentDensities": {
      "compact": true,
      "cozy": true
    },
    "models": {
      "i18n": {
        "type": "sap.ui.model.resource.ResourceModel",
        "settings": {
          "bundleName": "flexso.i18n.i18n",
          "bundleLocale": "nl"
        }
      },
      "eventModel": {
        "dataSource": "mainService",
        "preload": true,
        "settings": {
          "synchronizationMode": "None",
          "operationMode": "Server",
          "autoExpandSelect": true,
          "earlyRequests": true
        }
      },
      "v2model": {
        "dataSource": "v2model",
        "preload": true,
        "settings": {
          "synchronizationMode": "None",
          "operationMode": "Server",
          "autoExpandSelect": true,
          "earlyRequests": true
        }
      }
    },
    "resources": {
      "css": [
        {
          "uri": "css/style.css"
        }
      ]
    },
    "routing": {
      "config": {
        "routerClass": "sap.f.routing.Router",
        "type": "View",
        "viewType": "XML",
        "path": "flexso.view",
        "controlId": "flexibleColumnLayout",
        "transition": "slide",
        "bypassed": {
          "target": "TargetAccount"
        },
        "async": true
      },
      "routes": [
        
        
        {
          "name": "login",
          "pattern": "login",
          "target": ["Login"]
        },
        {
          "name": "register",
          "pattern": "register",
          "target": ["Register"]
        },
        {
          "name": "app",
          "pattern": "app",
          "target": ["TargetApp"]
        },
        {
          "name": "RouteEvent",
          "pattern": "Events({eventID})",
          "target": ["TargetEvent"]
        },
        {
          "name": "EventList",
          "pattern": "events",
          "target": ["TargetEventList"]
        },
        
        {
          "name": "eventManager",
          "pattern": "eventManager",
          "target": ["TargetEventManager"]
        },
        {
          "name": "sessionManager",
          "pattern": "sessionManager",
          "target": ["TargetSessionManager"]
        },
        {
          "name": "sessionManagerEditor",
          "pattern": "sessionManager/{sessieID}",
          "target": ["TargetSessionManagerEditor"]
        },
        {
          "pattern": ":layout:",
          "name": "list",
          "target": ["list", "detail"]
        },
        {
          "pattern": "detail/{eventID}/{layout}",
          "name": "detail",
          "target": ["list", "detail"]
        },
        {
          "name": "account",
          "pattern": "",
          "target": ["TargetAccount"]
        },
        {
          "pattern": "sessionDetail/{sessieID}",
          "name": "sessionDetail",
          "target": "sessionDetail"
        },
        {
          "name": "EditSession",
          "pattern": "Sessions/{sessieID}/edit",
          "target": ["EditSession"]
        }
      ],
      "targets": {
        "TargetApp": {
          "viewType": "XML",
          "viewPath": "flexso.view",
          "transition": "slide",
          "clearControlAggregation": false,
          "viewId": "App",
          "viewName": "App",
          "controlAggregation": "beginColumnPages"
        },
        "TargetEvent": {
          "viewType": "XML",
          "viewPath": "flexso.view",
          "viewName": "Account",
          "viewId": "Account"
        },
        "TargetEventManager": {
          "viewType": "XML",
          "viewPath": "flexso.view",
          "viewName": "EventManager",
          "viewId": "EventManager",
          "controlAggregation": "beginColumnPages",
          "viewLevel": 2
        },
        "TargetSessionManager": {
          "viewType": "XML",
          "viewPath": "flexso.view",
          "viewName": "SessionManager",
          "viewId": "SessionManager",
          "controlAggregation": "beginColumnPages",
          "viewLevel": 2
        },
        "TargetSessionManagerEditor": {
          "viewType": "XML",
          "viewPath": "flexso.view",
          "viewName": "SessionManager",
          "viewId": "SessionManager",
          "controlAggregation": "beginColumnPages",
          "viewLevel": 2
        },
        "TargetAccount": {
          "viewType": "XML",
          "viewPath": "flexso.view",
          "viewName": "List",
          "viewId": "List",
          "controlAggregation": "beginColumnPages"
        },
        "Login": {
          "name": "Login",
          "id": "Login",
          "controlAggregation": "beginColumnPages"
        },
        "Register": {
          "viewType": "XML",
          "transition": "slide",
          "controlAggregation": "beginColumnPages",
          "clearControlAggregation": false,
          "viewId": "Register",
          "viewName": "Register"
        },

        "list": {
          "name": "List",
          "id": "List",
          "controlAggregation": "beginColumnPages"
        },
        "detail": {
          "name": "Detail",
          "id":"Detail",
          "controlAggregation": "midColumnPages"
        },
        "sessionDetail": {
          "viewName": "SessionDetail",
          "controlAggregation": "beginColumnPages",
          "viewLevel": 2
        },
        "EditSession": {
          "viewType": "XML",
          "transition": "slide",
          "clearControlAggregation": false,
          "viewId": "EditSession",
          "viewName": "EditSession"
        }
      }
    },
    "rootView": {
      "viewName": "flexso.view.Account",
      "type": "XML",
      "async": true,
      "id": "app"

    }
  }
}
