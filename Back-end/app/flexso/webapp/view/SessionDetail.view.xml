<mvc:View
  xmlns:mvc="sap.ui.core.mvc"
  xmlns="sap.m"
  controllerName="flexso.controller.SessionDetail">
  <Page id="_IDGenPage1"
    title="Sessiedetails">
    <content>
      <VBox id="_IDGenVBox1">
        <Text id="_IDGenText1" text="{sessieID}" />
        <Text id="_IDGenText2" text="{naam}" />
        <Text id="_IDGenText3" text="{beschrijving}" />
        <Text id="_IDGenText4" text="{datum}" />
        <Text id="_IDGenText5" text="{beginTijd}" />
        <Text id="_IDGenText6" text="{eindTijd}" />
        <Text id="_IDGenText7" text="{lokaalnummer}" />
      </VBox>
    </content>
  </Page>
</mvc:View>
